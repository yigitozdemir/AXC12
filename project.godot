config_version=5

_global_script_classes=[{
"base": "Resource",
"class": &"Level",
"language": &"GDScript",
"path": "res://script/Level.gd"
}]
_global_script_class_icons={
"Level": ""
}

[application]

config/name="AXC12"
run/main_scene="res://scenes/main/welcome/welcome.tscn"
config/features=PackedStringArray("4.0", "Vulkan Mobile")
